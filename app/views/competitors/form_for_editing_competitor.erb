<h1>Edit Competitors</h1>

<form action = "/process_competitor_form/<%= @competitor.id %>" method = "post">
  <p>
    First Name:
    <input type = "text" name = "first_name" value = "<%= @competitor.first_name %>">
  </p>

  <p>
    Last Name:
    <input type = "text" name = "last_name" value = "<%= @competitor.last_name %>">
  </p>

  <p>
    School ID:
    <input type = "number" name = "school_id" value = "<%= @competitor.school_id %>">
  </p>

  <input type = "submit">
</form>