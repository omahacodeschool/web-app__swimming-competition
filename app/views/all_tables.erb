wf<table border="1">
  <thead>
    <tr>
      <th>School</th>
      <th>Conference</th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @schools.each do |school| %>
      <tr>
        <td><%= school.school_name %></td>
        <td><%= school.conference_id %></td>

      
        
      </tr>
  <% end %>
  </tbody>
</table>
<table border="1">
  <thead>
    <tr>
      <th>Swimmer</th>
      <th>School</th>
      <th>ID</th>
      <th>Event</th>

    </tr>
  </thead>

  <tbody>
  <% @swimmers.each do |swimmers| %>
      <tr>
        <td><%= swimmers.name %></td>
        <td><%= swimmers.name_school%></td>
      
    
        
      </tr>
    <% end %>
  </tbody>
</table>

<table border="1">
  <thead>
    <tr>
      <th>Event</th>
      <th>First Place Winner</th>
      <th>Second Place Winner</th>
      <th>Third Place Winner</th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.event_name %></td>
        <td><%= event.first_place_name %></td>
        <td><%= event.second_place_name %></td>
        <td><%= event.third_place_name %></td>
      
        
      </tr>
    <% end %>
  </tbody>
</table>



