<center><h1>TEAMS</h1></center>

<center>
<table border="1">
  <thead>
    <tr>
      <th>Team</th>
      <th>Conference ID</th>
      <th>Delete</th>
    </tr>
  </thead>

  <tbody>
    <% @info.each do |info| %>
      <tr>
        <td><%= info.team %></td>
        <td><%= info.conference_name %></td>
        <td><a href="/delete_team/<%= info.id %>">Delete</a></td>
      </tr>
    <% end %>
  </tbody>
</table>
</center>
