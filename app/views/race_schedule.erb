


<a href="/">Home</a><br>

<h1>Race Schedule</h1>




<% @events.each do |event| %>
  <p>
    <%= event.event_name %> <%= event.start_time %> 
  </p>

  <p>
    <form action ="/lock_event/<%= event.id %>" method="post">
      Lock This Event's Information
    <input type ="submit" value="Lock">
    </form><br>
  </p>


<% end%>
