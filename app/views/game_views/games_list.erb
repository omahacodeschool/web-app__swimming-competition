<h1>List of Games</h1>
<br>
<table border="1">
  <thead>
    <tr>
      <th>Game ID</th>
      <th>Game Name</th>
    <tr>
  </thead>

  <tbody>
    <% @games.each do |g| %>
      <tr>
        <td><center><%= g.id %></center></td>
        <td><a href="game/<%=g.id%>"><%= g.game_name %></a></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<br>
<a href="/">Home</a>