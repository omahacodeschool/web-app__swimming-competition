<h1> <%= @event.event_name %> </h1>
<table border="1">
  <thead>
    <tr>
      <th>Swimmer</th>
      <th>Swimmer's University</th>
      <th>University Conference</th>
    </tr>
  </thead>

  <tbody>
    <% @university.each do |y| %>
      <tr>
        <td>
          <a href="/swimmer_bio/<%= y.swimmer_id %>"> <%= y.swimmer %> </a>
        </td>
        <td>
          <%= y.university %>
        </td>
        <td>
          <%= y.conference %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<a href="/">Return Home</a>