Show form for updating Universities

<form action="/process_update_university/<%= @s.id %>" method="post">
  <p>
    University's Name
    <input type="text" name="new_university_name" 
      value="<%= @s.university_name %>">
  </p>
  <p>
    Conference ID
  <select name="conference_id">
  
    <option value="1">East</option>
    <option value="2">West</option>
    <option value="3">North</option>
    <option value="4">South</option>
  </select>
  </p>

  <input type="submit">
</form>


